{
  "name": "nyan-mode",
  "displayName": "nyan-mode",
  "description": "emacs nyan-mode on vscode",
  "version": "1.1.2",
  "publisher": "zakudriver",
  "repository": "https://github.com/zakudriver/nyan-mode-vscode",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "logo.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "nyan-cat": {
        "description": "nyan cat",
        "default": {
          "fontPath": "./nyan-cat.woff",
          "fontCharacter": ""
        }
      },
      "nyan-rainbow": {
        "description": "nyan tail",
        "default": {
          "fontPath": "./nyan-rainbow.woff",
          "fontCharacter": ""
        }
      },
      "nyan-space": {
        "description": "nyan space",
        "default": {
          "fontPath": "./nyan-space.woff",
          "fontCharacter": ""
        }
      }
    },
    "configuration": {
      "title": "nyan-mode",
      "properties": {
        "nyanMode.nyanDisable": {
          "type": "boolean",
          "default": false,
          "description": "Enable or disable nyan-mode."
        },
        "nyanMode.nyanAlign": {
          "type": "string",
          "default": "left",
          "enum": [
            "left",
            "right"
          ],
          "description": "Nyan-mode show at statusbar of 'left' or 'right'."
        },
        "nyanMode.nyanPriority": {
          "type": "number",
          "default": 0,
          "description": "Higher values mean nyan-mode should be shown more to the left."
        },
        "nyanMode.nyanColor": {
          "type": "string",
          "default": "#fff",
          "description": "Nyan-mode's color."
        },
        "nyanMode.nyanLength": {
          "type": "number",
          "default": 20,
          "description": "Nyan-mode's lenght."
        },
        "nyanMode.nyanDisplayPercent": {
          "type": "boolean",
          "default": false,
          "description": "If it's true, show position percent at nyan-mode of right."
        },
        "nyanMode.nyanHeader": {
          "type": "string",
          "default": "$(nyan-space)",
          "description": "String of nyan-mode's header."
        },
        "nyanMode.nyanFooter": {
          "type": "string",
          "default": "$(nyan-rainbow)",
          "description": "string of nyan-mode's footer."
        },
        "nyanMode.nyanAction": {
          "type": "string",
          "default": "line",
          "enum": [
            "line",
            "range"
          ],
          "description": "nyan-mode's move mode. The 'line' mode is based on the line where the cursor resides, and the 'range' is based on the view range."
        },
        "nyanMode.nyanFaceCurve": {
          "type": "Array",
          "default": [
            [
              "$(nyan-cat)",
              1
            ]
          ],
          "description": "The curve values of nyan's expression ( ^ｰ^)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  }
}
